<form action="/member/memberAddCon">
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">选择所属部门</label>
        <select class="form-control" name="bumenid">
            <option value="0">一级部门</option>
            <%- data %>>
        </select>
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">职务</label>
        <input type="text" class="form-control" id="" placeholder="职务" name="zhiwu">
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">姓名</label>
        <input type="text" class="form-control" id="" placeholder="name" name="name">
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">编号</label>
        <input type="text" class="form-control" id="" placeholder="number" name="numbers">
